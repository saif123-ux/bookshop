{
  "name": "bookshop",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "https://github.com/saif123-ux/bookshop",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds": "^9.2.1",
    "express": "^4"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.13.0",
    "@sap/cds-dk": ">=8"
  },
  "scripts": {
    "start": "cds run --with-mocks --in-memory",
    "dev": "cds watch",
    "build": "cds build",
    "deploy": "cds deploy"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": ":memory:"
        }
      },
      "auth": {
        "kind": "dummy"
      },
      "[production]": {
        "db": {
          "kind": "sqlite",
          "credentials": {
            "database": ":memory:"
          }
        },
        "auth": {
          "kind": "dummy"
        }
      }
    }
  }
}