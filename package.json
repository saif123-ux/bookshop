{
  "name": "bookshop",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "https://github.com/saif123-ux/bookshop",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/postgres": "^2.0.4",
    "@cap-js/sqlite": "^2.0.2",
    "@sap/cds": "^9.2.1",
    "@sap/cds-dk": ">=8",
    "express": "^4"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.13.0"
  },
 "scripts": {
    "start": "npx cds run --with-mocks --in-memory --exit-on-error false",
    "dev": "npx cds watch",
    "build": "npx cds build",
    "deploy": "npx cds deploy",
    "render-build": "npm install"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": ":memory:"
        }
      },
      "auth": {
        "kind": "dummy"
      },
      "[production]": {
        "db": {
          "kind": "sqlite",
          "credentials": {
            "database": ":memory:"
          }
        },
        "auth": {
          "kind": "dummy"
        }
      }
    }
  }
}
